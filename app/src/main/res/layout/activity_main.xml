<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_main"
    tools:context=".MainActivity"
    >


    <Button
        android:id="@+id/Logout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"

        android:text="Button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/IBDInformasi" />

    <ImageButton
        android:id="@+id/ib_Pemasukan"
        android:layout_width="120dp"
        android:layout_height="110dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="292dp"
        android:background="@drawable/btn_pemasukan_input"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/ib_Pengeluaran"
        android:layout_width="120dp"
        android:layout_height="110dp"
        android:layout_marginTop="292dp"
        android:layout_marginEnd="64dp"
        android:background="@drawable/btn_pengeluaran_input"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/IBDInformasi"
        android:layout_width="150dp"
        android:layout_height="112dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/btn_informasi"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/IBDPengeluaran" />

    <ImageButton
        android:id="@+id/IBDPengeluaran"
        android:layout_width="150dp"
        android:layout_height="110dp"
        android:layout_marginTop="68dp"
        android:layout_marginEnd="32dp"
        android:background="@drawable/btn_data_pengeluaran"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ib_Pengeluaran" />

    <ImageButton
        android:id="@+id/IBDPemasukan"
        android:layout_width="150dp"
        android:layout_height="110dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="68dp"
        android:background="@drawable/btn_data_pemasukan"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ib_Pemasukan" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:text="Menu"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ib_Pemasukan" />


</androidx.constraintlayout.widget.ConstraintLayout>
